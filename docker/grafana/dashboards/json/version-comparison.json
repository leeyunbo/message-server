      "gridPos": { "x": 0, "y": 1, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "fillOpacity": 20, "lineWidth": 2 },
          "thresholds": { "mode": "absolute", "steps": [{ "value": null, "color": "green" }, { "value": 10, "color": "yellow" }, { "value": 50, "color": "red" }] }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "V1 Blocking" }, "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }] },
