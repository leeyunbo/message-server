      },
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max", "lastNotNull"] } },
      "targets": [
        { "refId": "A", "expr": "histogram_quantile(0.95, rate(netty_eventloop_lag_ms_bucket{mode=\"blocking\"}[1m]))", "legendFormat": "V1 Blocking" },
